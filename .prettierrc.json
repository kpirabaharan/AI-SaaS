{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "arrowParens": "avoid",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"]
}
